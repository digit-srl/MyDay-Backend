<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">

    <title>@ViewData["Title"] | Digital Arianna Dashboard</title>

    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link href="//fonts.googleapis.com/css?family=Raleway:400,300,600" rel="stylesheet" type="text/css" />

    <link rel="stylesheet" href="~/skeleton-2.0.4/normalize.css" />
    <link rel="stylesheet" href="~/skeleton-2.0.4/skeleton.css" />
    <link rel="stylesheet" href="~/style.css" />

    @RenderSection("Head")
</head>

<body>
    <header>
        <div class="container">
            <div class="row">
                <div class="twelve columns">
                    <h1 class="centered">Dashboard</h1>
                </div>
            </div>
        </div>
    </header>

    <section>
        <div class="container">
            <div class="row">
                <div class="twelve columns">
                    @RenderBody()
                </div>
            </div>
        </div>
    </section>

    <footer>
        <div class="container">
            <div class="row">
                <div class="twelve columns">
                    <p>diAry &mdash; Digital Arianna &mdash; @(DateTime.UtcNow.Year)</p>
                    <p><a href="https://covid19app.uniurb.it/">More information</a></p>
                </div>
            </div>
        </div>
    </footer>

</body>
</html>
